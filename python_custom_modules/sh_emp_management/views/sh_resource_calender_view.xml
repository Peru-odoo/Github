<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sh_resource_calender_form_view" model="ir.ui.view">
        <field name="name">sh.resource.calender.form.view</field>
        <field name="model">sh.resource.calendar</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="active" />       
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="sh_resource_calender_list_view" model="ir.ui.view">
        <field name="name">sh.resource.calender.list.view</field>
        <field name="model">sh.resource.calendar</field>
        <field name="arch" type="xml">
            <list>
                <field name="name" />
                <field name="active" />
            </list>
        </field>
    </record>
    <record id="sh_resource_calender_action" model="ir.actions.act_window">
        <field name="name">Calender</field>
        <field name="res_model">sh.resource.calendar</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>
