<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="sh_age_category_view_form" model="ir.ui.view">
        <field name="name">sh.age.category.view.form</field>
        <field name="model">sh.age.category</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>        
                        <field name="min_age"/>
                        <field name="max_age"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="sh_age_category_view_list" model="ir.ui.view">
        <field name="name">sh.age.category.view.list</field>
        <field name="model">sh.age.category</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>        
                <field name="min_age"/>
                <field name="max_age"/>
            </list>
        </field>
    </record>

    <record id="sh_age_category_action" model="ir.actions.act_window">
        <field name="name">Age Category</field>
        <field name="res_model">sh.age.category</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>