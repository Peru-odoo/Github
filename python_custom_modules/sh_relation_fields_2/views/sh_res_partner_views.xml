<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="sh_res_partner_view_form" model="ir.ui.view">
        <field name="name">sh.res.partner.view.form</field>
        <field name="model">sh.res.partner</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="city"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="sh_res_partner_view_list" model="ir.ui.view">
        <field name="name">sh.res.partner.view.list</field>
        <field name="model">sh.res.partner</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="city"/>
            </list>
        </field>
    </record>

    <record id="sh_res_partner_action" model="ir.actions.act_window">
        <field name="name">Res Partner</field>
        <field name="res_model">sh.res.partner</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>